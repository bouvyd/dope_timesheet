{
    "manifest_version": 3,
    "name": "Dope Timesheet",
    "version": "0.1",
    "description": "A Chrome extension for managing timesheets as an Odoo Employee",
    "action": {
        "default_popup": "index.html"
    },
"permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://www.odoo.com"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}